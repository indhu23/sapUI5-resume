<core:View xmlns:core="sap.ui.core"
		   xmlns="sap.uxap"
		   xmlns:layout="sap.ui.layout"
		   xmlns:m="sap.m"
		   xmlns:blockcolor="resume.SharedBlocks"
		   height="100%"
		>

	<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false">
		<headerTitle>
			<ObjectPageHeader objectTitle="Section sample">
			</ObjectPageHeader>
		</headerTitle>

		<headerContent>
			<m:ObjectAttribute title="" text="This example explains the rules for the rendering of sections"/>
		</headerContent>

		<sections>
			<ObjectPageSection title="About">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<blockcolor:BlockBlueT1 id="bbt1"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="Experience">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<blockcolor:BlockBlueT1 id="bbt2"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="Education">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<blockcolor:BlockBlueT1 id="bbt3"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="Skills">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<blockcolor:BlockBlueT1 id="bbt4"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="Interests">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<blockcolor:BlockBlueT1 id="bbt5"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>

	</ObjectPageLayout>
</core:View>
